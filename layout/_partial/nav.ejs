<nav class="main-navigation">
    <div class="container">
        <div class="row">
            <div class="col-sm-12">
                <div class="navbar-header"><span class="nav-toggle-button collapsed pull-right" data-toggle="collapse" data-target="#main-menu" id="mnav">
                    <span class="sr-only"></span>
                        <i class="fa fa-bars"></i>
                    </span>
                    <a class="navbar-brand" href="<%= config.url %>"><%= config.title %></a>
                </div>
                <div class="collapse navbar-collapse" id="main-menu">
                    <ul class="menu">
                        <% theme.menu.forEach(function (item) {%>
                            <li role="presentation" class="text-center">
                                <a href="<%- item.url%>"><i class="fa <%- item.icon%>"></i><%= __(item.page+'') %></a>
                            </li>
                        <%})%>
                    </ul>
                </div>
            </div>
        </div>
    </div>
</nav>
<% if(theme.fork_me_on_github.enabled){ %>
<a href="<%- theme.fork_me_on_github.url%>">
    <img style="position: absolute; top: 0; right: 0; border: 0;"
        src="/img/forkme.png"
        alt="Fork me on GitHub">
</a>
<% } %>